{
  "name": "QRcodeURL",
  "version": "1.0.0",
  "manifest_version":2,
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "description": "URL生成二维码",
  "icons": {
      "128":"icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "URL二维码",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ]
}